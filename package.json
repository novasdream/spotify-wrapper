{
  "name": "spotify-wrapper",
  "version": "1.0.0",
  "description": "A wrapper para o servi√ßo spotify",
  "main": "index.js",
  "scripts": {
    "clear": "rimraf lib",
    "build": "npm run clear && ./node_modules/.bin/babel --out-dir lib src",
    "build:watch": "npm run build -- --watch",
    "build:umd": "./node_modules/.bin/webpack --output-filename spotify-wrapper.umd.js",
    "build:umd:min": "./node_modules/.bin/webpack --output-filename spotify-wrapper.umd.js -p",
    "build:all": "npm run build && npm run build:umd && npm run build:umd:min",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willianjusten/js-tdd-course.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Willian Justen <willianjustenqui@gmail.com> (https://willianjusten.com.br/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/willianjusten/js-tdd-course/issues"
  },
  "homepage": "https://github.com/willianjusten/js-tdd-course#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.14.0",
    "growl": ">=1.10.0",
    "lodash": ">=4.17.5",
    "husky": "^0.11.9",
    "mocha": "^3.5.3",
    "node-fetch": "^2.3.0",
    "nyc": "^10.3.2",
    "rimraf": "^2.6.2",
    "sinon": "^7.2.2",
    "sinon-chai": "^3.3.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {}
}
